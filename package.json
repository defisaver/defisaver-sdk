    "@types/node": "^18.11.8",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.57.0",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^8.4.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.20",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^4.10.0"
  }
